define isPrime(int x) -> int {

    if x == 0 || x == 1 return false;

    mut int i = 2;

    while i <= x / 2 {
        if x % i == 0 {
            return false;
        }
        i++;
    }

    return true;
}

define main() -> int {

    int x = in "Enter a number: ";

    if isPrime(x) {
        out "You entered a prime number\n";
    } else {
        out "You entered a composite number\n";
    }

    return 0;
}